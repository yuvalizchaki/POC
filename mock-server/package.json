{
  "name": "mock-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "dev": "concurrently \"npm run tsc:watch\" \"npm run watch-node\" \"npm run watch-public\"",
    "watch-public": "ncp public dist/public --watch",
    "watch-node": "node --watch --watch-preserve-output dist/app.js",
    "tsc:watch": "tsc --watch",
    "start": "node dist/app.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "concurrently": "^8.2.2",
    "ncp": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
